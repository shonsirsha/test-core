{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
       "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions":{
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-multi-str": "off"
    }
}
